    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "eslint-config-prettier": "^10",
    "husky": "^9",
    "lint-staged": "^16",
    "npm-run-all2": "^8",
    "prettier": "^3",
    "prettier-plugin-packagejson": "^2",
    "typescript": "^5",
    "typescript-eslint": "^8"
  },
  "packageManager": "yarn@1.22.22"
